{
  "name": "scenery",
  "version": "1.0.0",
  "description": "3D environments rendered with Typescript and Three.js",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "clean": "clean:dist",
    "clean:dist": "rm -rf ./dist",
    "build": "run-s build:js",
    "build:js": "npm run build:js:cjs && npm run build:js:esm && npm run build:js:umd",
    "build:js:cjs": "tsc -p tsconfig.cjs.json",
    "build:js:esm": "tsc -p tsconfig.esm.json",
    "build:js:umd": "tsc -p tsconfig.umd.json",
    "build:css": "./scripts/buildCSS",
    "lint": "eslint .",
    "test": "npx jest"
  },
  "keywords": [
    "typescript",
    "threejs",
    "3d"
  ],
  "author": "Victor Fortman",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@types/three": "^0.137.0",
    "three": "^0.137.5"
  }
}
